{
	"geode": "4.8.0",
	"gd": {
		"win": "2.2074"
	},
	"id": "kevadroz.practicecheckpointpermanence",
	"name": "Practice Checkpoint Permanence",
	"version": "v1.0.0",
	"developer": "Kevadroz",
	"description": "Make checkpoints that persist across sessions",
	"dependencies": {
		"geode.custom-keybinds": ">=v1.10.0",
		"geode.node-ids": ">=v1.20.0",
		"sabe.persistenceapi": ">=v1.0.0"
	},
	"resources": {
		"spritesheets": {
			"MainSheet": [
				"resources/mainSheet/*.png"
			]
		}
	},
	"settings": {
		"inactive-checkpoint-opacity": {
			"name": "Inactive Checkpoint Opacity",
			"type": "float",
			"default": 0.4,
			"min": 0.0,
			"max": 1.0,
			"control": {
				"slider": true,
				"slider-step": 0.01,
				"arrows": true,
				"arrow-step": 0.1
			}
		}
	}
}
